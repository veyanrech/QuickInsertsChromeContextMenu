{
    "name": "Context Menus Sample",
    "description": "Uses the chrome.contextMenus API to customize the context menu.",
    "version": "0.1",
    "permissions": ["contextMenus","activeTab","scripting","storage"],
    "background": {
      "service_worker": "sample.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./src/build/content.js"]
      }
    ],
    "manifest_version": 3
  }